{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite-supabase-url",
    "VITE_SUPABASE_ANON_KEY": "@vite-supabase-anon-key",
    "DODO_PAYMENTS_API_KEY": "@dodo-payments-api-key",
    "DODO_WEBHOOK_SECRET": "@dodo-webhook-secret",
    "VITE_DODO_PRODUCT_ID_PRO": "@vite-dodo-product-id-pro",
    "VITE_DODO_PRODUCT_ID_ADVANCED": "@vite-dodo-product-id-advanced",
    "VITE_DODO_PRODUCT_ID_ENTERPRISE": "@vite-dodo-product-id-enterprise"
  }
}

